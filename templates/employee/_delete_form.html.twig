<form id="form{{employee.id}}" class="display-contents" method="post" action="{{ path('app_employee_delete', {'id': employee.id}) }}">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ employee.id) }}">
	<a data-id="form{{employee.id}}" class="delete-data btn btn-danger btn-sm delete-btn" title="Supprimer">
		<i class="fas fa-trash"></i>
        Supprimer
    </a>
</form>
